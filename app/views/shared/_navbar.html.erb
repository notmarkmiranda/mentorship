      MENTOR<span>SHIP</span>
    <% end %>
  </div>

  <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <ul class="nav navbar-nav navbar-right">
      <% if !current_user %>
        <li><%= link_to "How does it work?", how_path %></li>
      <% end %>
      <li><%= link_to "Browse Students", students_path %></li>
      <li><%= link_to "Browse Mentors", mentors_path %></li>
